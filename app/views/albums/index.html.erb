<p id="notice"><%= notice %></p>

<h1>Listing Albums</h1>

<table id="albums-table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Releasedate</th>
      <th>Genre</th>
      <th>Amountsold</th>
      <th>Billboard200peak</th>
      <th>Numberofsingles</th>
    </tr>
  </thead>

  <tbody>
    <% @albums.each do |album| %>
      <tr>
        <td><%= album.title %></td>
        <td><%= album.releaseDate %></td>
        <td><%= album.genre %></td>
        <td><%= album.amountSold %></td>
        <td><%= album.billboard200Peak %></td>
        <td><%= album.numberOfSingles %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Album', new_album_path %>
