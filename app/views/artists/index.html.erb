<p id="notice"><%= notice %></p>

<h1>Listing Artists</h1>

<table id="artists-table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Home Country</th>
      <th>Home Region</th>
      <th>Gender</th>
      <th>Race</th>
      <th>Debut Year</th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr class="clickable-row" data-href="<%= artist_url(artist) %>">
        <td><%= artist.name %></td>
        <td><%= artist.age %></td>
        <td><%= artist.homeCountry %></td>
        <td><%= artist.homeState %></td>
        <td><%= artist.gender %></td>
        <td><%= artist.race %></td>
        <td><%= artist.debutYear %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist', new_artist_path %>
